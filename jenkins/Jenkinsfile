pipeline
{
    agent any
    tools {
      maven 'Maven 3.9'
    }
    parameters {
      string defaultValue: 'RAJI', name: 'LASTNAME'
    }

    environment{
        NAME = "kerahn"
    }

    stages
    {
     stage('build')
        {
        steps{
            sh 'mvn clean package'
            echo "hello $NAME  ${params.LASTNAME}"
             }

        post {
          success {
                archiveArtifacts artifacts: '**/target/*.jar'
                }
            }
        }
    }
}


